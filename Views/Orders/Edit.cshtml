@model CoreReactLab.Models.Order

@{
    Layout = "_Layout";
}


<h4>Заказ</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="id" />
            <div class="form-group">
                <label asp-for="adminId" class="control-label"></label>
                <select asp-for="adminId" class="form-control" asp-items="ViewBag.adminId"></select>
                <span asp-validation-for="adminId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="workerId" class="control-label"></label>
                <select asp-for="workerId" class="form-control" asp-items="ViewBag.workerId"></select>
                <span asp-validation-for="workerId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="typeWorkId" class="control-label"></label>
                <select asp-for="typeWorkId" class="form-control" asp-items="ViewBag.typeWorkId"></select>
                <span asp-validation-for="typeWorkId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FIO" class="control-label"></label>
                <input asp-for="FIO" class="form-control" />
                <span asp-validation-for="FIO" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VinNum" class="control-label"></label>
                <input asp-for="VinNum" class="form-control" />
                <span asp-validation-for="VinNum" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CarName" class="control-label"></label>
                <input asp-for="CarName" class="form-control" />
                <span asp-validation-for="CarName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TelNum" class="control-label"></label>
                <input asp-for="TelNum" class="form-control" />
                <span asp-validation-for="TelNum" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DateTime" class="control-label"></label>
                <input asp-for="DateTime" class="form-control" />
                <span asp-validation-for="DateTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Сохранить" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Назад</a>
</div>

